{
    "version": 2,
    "rewrites": [
        {
            "source": "/",
            "destination": "/home.html"
        },
        {
            "source": "/member",
            "destination": "/components/page/member.html"
        },
        {
            "source": "/news",
            "destination": "/components/page/news.html"
        },
        {
            "source": "/schedule",
            "destination": "/components/page/schedule.html"
        },
        {
            "source": "/sorter",
            "destination": "/components/game/sorter.html"
        },
        {
            "source": "/guess",
            "destination": "/components/game/guess.html"
        },
        {
            "source": "/share",
            "destination": "/components/page/share.html"
        },
        {
            "source": "/guess/ingame",
            "destination": "/components/game/guess-ingame.html"
        },
        {
            "source": "/ingames",
            "destination": "/components/page/ingames.html"
        },
        {
            "source": "/mypage",
            "destination": "/components/login/mypage.html"
        },
        {
            "source": "/login",
            "destination": "/components/login/login.html"
        },
        {
            "source": "/about",
            "destination": "/components/page/about.html"
        },
        {
            "source": "/notfound",
            "destination": "/components/page/notfound.html"
        },
        {
            "source": "/maintenance",
            "destination": "/middleware/maintenance.html"
        },
        {
            "source": "/news/:id",
            "destination": "/components/detail/news.html?id=:id"
        },
        {
            "source": "/member/:id",
            "destination": "/components/detail/member.html?id=:id"
        },
        {
            "source": "/components/detail/:slug",
            "destination": "/components/detail/:slug.html"
        },
        {
            "source": "/middleware/:slug",
            "destination": "/middleware/:slug.html"
        },
        {
            "source": "/components/page/:slug",
            "destination": "/components/page/:slug.html"
        }
    ],
    "errorPages": {
        "500": "/components/page/500.html"
    }
}
